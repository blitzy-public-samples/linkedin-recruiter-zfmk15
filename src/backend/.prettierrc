{
  "printWidth": 100,
  "tabWidth": 2,
  "useTabs": false,
  "semi": true,
  "singleQuote": true,
  "trailingComma": "es5",
  "bracketSpacing": true,
  "arrowParens": "always",
  "endOfLine": "lf",
  "overrides": [
    {
      "files": "*.py",
      "options": {
        "tabWidth": 4,
        "printWidth": 88
      }
    },
    {
      "files": "*.java",
      "options": {
        "tabWidth": 4,
        "printWidth": 120
      }
    }
  ]
}